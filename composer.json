{
    "name": "mikulas/lattecs",
    "license": "MIT",
    "description": "Coding standard validator for Latte templates",
    "keywords": ["latte", "nette", "cs", "coding standard", "checker", "console", "tool", "validator"],
    "minimum-stability": "RC",
    "authors": [
        {
            "name": "Mikulas Dite",
            "email": "mikulas@dite.pro"
        }
    ],
    "require": {
        "symfony/console": "~2.4.4",
        "latte/latte": "~2.2-rc2",
        "nette/finder": "~2.2-rc1"
    },
    "require-dev": {
        "nette/tester": "~1.1.0"
    },
    "autoload": {
        "psr-0": {
            "Mikulas\\LatteCS\\": "src/"
        }
    },
    "bin": [
        "bin/lattecs"
    ]
}
